{
  "problemMatcher": [
    {
      "owner": "phpunit",
      "pattern": [
        {
          "regexp": "^(.*Failed\\sasserting\\sthat.*)$",
          "message": 1
        },
        {
          "regexp": "^[\\s\\S]*$"
        },
        {
          "regexp": "^(.*):(\\d+)$",
          "file": 1,
          "line": 2
        }
      ]
    }
  ]
}