{
  "problemMatcher": [
    {
      "owner": "phpunit",
      "pattern": [
        {
          "regexp": "^(.*Failed\\sasserting\\sthat[\\s\\S]*?)\\n{2}",
          "message": 1
        },
        {
          "regexp": "^\\s*$"
        },
        {
          "regexp": "^(.*):(\\d+)$",
          "file": 1,
          "line": 2
        }
      ]
    }
  ]
}