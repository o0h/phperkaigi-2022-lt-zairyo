{
  "problemMatcher": [
    {
      "owner": "phpunit",
      "pattern": [
        {
          "regexp": "(\\d+\\)[\\s\\S]*?Failed\\sasserting\\sthat[\\s\\S]*?)^$",
          "message": 1
        },
        {
          "regexp": "^(.*):(\\d+)$",
          "file": 1,
          "line": 2
        }
      ]
    }
  ]
}