{
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.5",
    "squizlabs/php_codesniffer": "^3.6",
    "vimeo/psalm": "^4.22",
    "psalm/plugin-phpunit": "^0.16.1",
    "friendsofphp/php-cs-fixer": "^3.8",
    "phan/phan": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "stan": "phpstan",
    "cs": "phpcs"
  },
  "config": {
    "platform": {
      "php": "8.1"
    }
  }
}
